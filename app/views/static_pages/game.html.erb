<!DOCTYPE html>

<html>
	<head>
		<title>Gridworkz</title>
		<link rel='stylesheet' type='text/css' href='custom.css.scss'/>
	</head>
 	<body> 
 		<canvas id="a" width="1100" height="600">
 			This text is displayed if your browser does not support HTML5 Canvas.
 		</canvas>
 		<script >
 			var a_canvas = document.getElementById("a");
			var context = a_canvas.getContext("2d");
				//draw
				var x = 20;
				var y = 20;
				for(var i = 0; i < 3; i++){
					x = 20;
					for(var j = 0; j < 3; j++){

						context.stroke();
						context.fillStyle = "black";
						context.fill();
						context.lineWidth = 2;
						context.beginPath();
						context.strokeRect(x, y, 40, 40);
						context.closePath();
						x = x + 40;
					}
					y = y + 40;
				}
				

				//context.font = "30px Garamond";
				//context.fillText("Hello, World!",15,175);

 		</script> 	
 	</body>
</html>